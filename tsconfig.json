{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "jsx": "preserve",
    "importHelpers": true,
    "moduleResolution": "node",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "baseUrl": ".",
    "types": [
      "vite-plugin-vue-layouts/client",
      "vite-plugin-pages/client",
      "vite/client",
      "unplugin-vue2-script-setup/types"
    ],
    "paths": {
      "@/*": ["./src*"]
    },
    "lib": ["esnext", "dom", "dom.iterable", "scripthost"]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.vue",
    "tests/**/*.ts",
    "tests/**/*.tsx"
  ],
  "files": [
    "types/shims-tsx.d.ts",
    "types/shims-vue.d.ts",
    "types/vuedraggable.d.ts",
    "types/vue-flag-icon.d.ts",
    "types/vue-apple-signin.d.ts",
    "types/vue-share-buttons.d.ts",
    "types/vue-simple-calendar.d.ts",
    "types/vue-confetti.d.ts",
    "types/cnf-qrcode.d.ts",
    "types/vue-cookie-law.d.ts",
    "types/vue-swipe-actions.d.ts",
    "types/vue-plugin-load-script.d.ts",
    "types/vue-countdown.d.ts",
    "types/@borodutch-labs/v-hotkey.d.ts"
  ]
}
